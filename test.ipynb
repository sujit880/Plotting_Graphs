    "\n",
    "# Create a tensor with negative values\n",
    "tensor = torch.tensor([-1, 2, -3], dtype=float)\n",
    "\n",
    "# Calculate L1 norm along dimension 0 (vector norm)\n",
    "l1_norm = torch.norm(tensor, )\n",
    "\n",
    "print(l1_norm)  # Output: tensor(-6.)\n"
   ]
